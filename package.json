{
  "name": "co-http-test",
  "version": "0.1.0",
  "author": "Max Millien",
  "description": "http testing made easy",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/co-http-test.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "test": "npm run build && tape ./test"
  },
  "keywords": [
    "koa",
    "api",
    "http",
    "utils",
    "requests",
    "co",
    "test",
    "httpTest",
    "generators",
    "convenience"
  ],
  "dependencies": {
    "rain-util-http": "*"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-3": "*",
    "co-tape": "*",
    "tape": "4.x",
    "tape-catch": "1.x"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node5",
      "stage-3"
    ]
  }
}
