{
  "name": "co-http-test",
  "version": "0.3.0",
  "author": "Max Millien",
  "description": "http request validations made easy",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/co-http-test.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "postbuild": "npm test",
    "test": "tape ./test",
    "watch": "watch 'npm run build' lib test"
  },
  "keywords": [
    "koa",
    "api",
    "http",
    "utils",
    "requests",
    "validation",
    "co",
    "test",
    "tape",
    "testing",
    "qa",
    "httpTest",
    "generators",
    "convenience"
  ],
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-3": "*",
    "deep-equal": "*",
    "co-tape": "*",
    "tape": "*",
    "tape-catch": "*",
    "rain-util-http": "*",
    "watch": "*"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node5",
      "stage-3"
    ]
  }
}
